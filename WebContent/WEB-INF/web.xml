<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Map</display-name>
  <welcome-file-list>
    <welcome-file>main.html</welcome-file>
  </welcome-file-list>

<servlet>
	<display-name>UserServlet</display-name>
	<servlet-name>UserServlet</servlet-name>
	<servlet-class>com.zr.service.UserServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>UserServlet</servlet-name>
	<url-pattern>/UserServlet</url-pattern>
</servlet-mapping>
<servlet>
	<display-name>MesServlet</display-name>
	<servlet-name>MesServlet</servlet-name>
	<servlet-class>com.zr.service.MesServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>MesServlet</servlet-name>
	<url-pattern>/MesServlet</url-pattern>
</servlet-mapping>
<servlet>
	<display-name>FinServlet</display-name>
	<servlet-name>FinServlet</servlet-name>
	<servlet-class>com.zr.service.FinServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>FinServlet</servlet-name>
	<url-pattern>/FinServlet</url-pattern>
</servlet-mapping>
</web-app>