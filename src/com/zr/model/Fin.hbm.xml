<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    <hibernate-mapping>
    	<class name="com.zr.model.Fin" table="tb_fin">
    		<id name="finId" type="java.lang.String" column="fin_id" length="50">
    			<generator class="uuid"/>
    		</id>
    		<property name="finSend" type="java.lang.String">
    			<column name="fin_senduser" length="50"/>
    		</property>
    		<property name="finGet" type="java.lang.String">
    			<column name="fin_getuser" length="50"/>
    		</property>
    	</class>
    </hibernate-mapping>