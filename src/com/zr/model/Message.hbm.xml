<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    <hibernate-mapping>
    	<class name="com.zr.model.Message" table="tb_mes">
    		<id name="mesId" type="java.lang.String" column="mes_id" length="50">
    			<generator class="uuid"/>
    		</id>
    		<property name="mesSend" type="java.lang.String">
    			<column name="mes_senduser" length="50"/>
    		</property>
    		<property name="mesGet" type="java.lang.String">
    			<column name="mes_getuser" length="50"/>
    		</property>
    	</class>
    </hibernate-mapping>